// Honeycomb Vision theme definition
// Copy of your custom theme JSON
{
  "name": "Honeycomb Vision",
  "type": "dark",
  "colors": {
    "editor.background": "#1a1026",
    "editor.foreground": "#ffe066",
    "editorCursor.foreground": "#ffd700",
    "editor.lineHighlightBackground": "#3d2067",
    "editor.selectionBackground": "#ffd70055",
    "editor.inactiveSelectionBackground": "#3d2067",
    "editorIndentGuide.background": "#2d1743",
    "editorIndentGuide.activeBackground": "#ffd700",
    "activityBar.background": "#2d1743",
    "sideBar.background": "#2d1743",
    "statusBar.background": "#2d1743",
    "tab.activeBackground": "#1a1026",
    "tab.inactiveBackground": "#2d1743",
    "tab.activeForeground": "#ffe066",
    "tab.inactiveForeground": "#b39ddb",
    "titleBar.activeBackground": "#3d2067",
    "titleBar.activeForeground": "#ffd700",
    "titleBar.inactiveBackground": "#2d1743",
    "titleBar.inactiveForeground": "#ffe066",
    "button.background": "#ffd700",
    "button.foreground": "#3d2067",
    "button.hoverBackground": "#ffe066",
    "badge.background": "#ffd700",
    "badge.foreground": "#3d2067",
    "list.activeSelectionBackground": "#ffd70033",
    "list.inactiveSelectionBackground": "#3d206733",
    "list.hoverBackground": "#ffe06633",
    "editorLineNumber.foreground": "#b39ddb",
    "editorLineNumber.activeForeground": "#ffd700"
  },
  "tokenColors": [
    {
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": { "foreground": "#b39ddb" }
    },
    {
      "scope": ["string", "constant.other.symbol"],
      "settings": { "foreground": "#ffe066" }
    },
    {
      "scope": ["variable", "meta.definition.variable"],
      "settings": { "foreground": "#ffd700" }
    },
    {
      "scope": ["keyword", "storage.type"],
      "settings": { "foreground": "#a259f7" }
    },
    {
      "scope": ["entity.name.function", "support.function"],
      "settings": { "foreground": "#ffb300" }
    },
    {
      "scope": ["constant.numeric", "constant.language"],
      "settings": { "foreground": "#f6c177" }
    },
    {
      "scope": ["entity.name.class", "support.class"],
      "settings": { "foreground": "#a259f7" }
    },
    {
      "scope": ["punctuation", "meta.brace"],
      "settings": { "foreground": "#ffd700" }
    }
  ]
}
